{
  "name": "cjs2es6",
  "version": "0.1.0",
  "description": "Automatic migration from CommonJS to ES6 modules",
  "main": "index.mjs",
  "files": [
    "package.json",
    "index.mjs",
    "CHANGELOG.md",
    "README.md",
    "LICENSE.txt"
  ],
  "engines": {
    "node": "16 || 14 || 18",
    "npm": "8 || 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tripu/cjs2es6.git"
  },
  "keywords": [
    "JavaScript",
    "export",
    "modules",
    "JS",
    "ES6",
    "CommonJS",
    "convert",
    "ECMAScript",
    "conversion",
    "import",
    "replace-require-with-import",
    "require2import"
  ],
  "bin": {
    "cjs2es6": "./index.mjs"
  },
  "contributors": [
    {
      "name": "James J. Womack",
      "url": "https://twitter.com/james_womack"
    },
    {
      "name": "Ole Morten Didriksen",
      "url": "https://github.com/oledid"
    },
    {
      "name": "luoage",
      "url": "https://github.com/luoage"
    },
    {
      "name": "tripu",
      "url": "https://tripu.info/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tripu/cjs2es6/issues"
  },
  "homepage": "https://github.com/tripu/cjs2es6",
  "dependencies": {
    "globby": "^13.1.2"
  },
  "scripts": {
    "clean": "rm -rf test/output/ && cp -r test/input/ test/output/",
    "process-samples": "node . test/output/t*.js test/output/**/*.js && diff -qr test/output test/expected",
    "test": "npm run clean && npm run process-samples",
    "prepack": "cp .github/README.md .github/LICENSE.txt ./",
    "postpack": "rm -f README.md LICENSE.txt"
  }
}
